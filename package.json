{
  "name": "mac-video-merger",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --mac --dir",
    "dist": "electron-builder --mac"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^33.0.0",
    "electron-builder": "^25.0.0"
  },
  "dependencies": {
    "fluent-ffmpeg": "^2.1.3"
  },
  "build": {
    "appId": "com.example.video-merger",
    "mac": {
      "category": "public.app-category.video",
      "target": ["dmg", "zip"],
      "icon": "assets/icon.icns"
    }
  }
}